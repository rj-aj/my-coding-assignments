<body>
    <div class="container">
        <h1> Counter </h1>
        <p> page visited <%= counter %>  times. </p>
        <div class="row">
            <div class="col-md-5">
             <form action="/two" method="POST">
                <button class='btn btn-primary' type="submit"> Incremet Count By 2 </button>
                </form>
            </div>
            <div class="col-md-5">
                <form action="/reset" method="POST">
                    <button class="btn btn-warning" type="submit"> Reset Count </button>
                </form>
            </div>
        </div>
    </div>

</body>

</html>